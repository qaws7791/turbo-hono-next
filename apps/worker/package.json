{
  "name": "worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development DOTENV_CONFIG_PATH=../api/.env tsx watch src/index.ts",
    "build": "cross-env NODE_ENV=production node esbuild.config.mjs",
    "start": "node dist/index.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "@repo/ai": "workspace:*",
    "@repo/core-adapters": "workspace:*",
    "@repo/core": "workspace:*",
    "@repo/database": "workspace:*",
    "@repo/queue-bullmq": "workspace:*",
    "@repo/storage-r2": "workspace:*",
    "dotenv": "^17.2.2",
    "neverthrow": "^8.2.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "cross-env": "^10.1.0",
    "esbuild": "^0.27.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
