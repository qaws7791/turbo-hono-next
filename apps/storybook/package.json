{
  "name": "storybook",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "check-types": "tsc --noEmit",
    "lint": "eslint stories --ext .ts,.tsx",
    "lint:fix": "eslint stories --ext .ts,.tsx --fix",
    "format": "prettier --write \"stories/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"stories/**/*.{ts,tsx}\"",
    "serve": "npx http-server storybook-static -p 6007 -o",
    "clean": "rm -rf storybook-static"
  },
  "dependencies": {
    "@repo/ui": "workspace:*",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-aria-components": "latest",
    "tailwind-variants": "^1.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "autoprefixer": "^10.4.21",
    "prop-types": "^15.8.1",
    "storybook": "8.6.14",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.0",
    "typescript": "5.8.3",
    "vite": "^6.3.5",
    "@repo/config": "workspace:*"
  }
}
