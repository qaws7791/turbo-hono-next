{
  "name": "web",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 4000",
    "start": "vite --port 4000",
    "build": "vite build && tsc",
    "check-types": "tsc --noEmit",
    "serve": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "schema:generate": "pnpm --filter @repo/api-spec docs:generate && openapi-typescript ../../packages/api-spec/dist/openapi.json --output ./src/api/schema.ts"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.93",
    "@internationalized/date": "^3.10.0",
    "@repo/ai-types": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tailwindcss/vite": "catalog:",
    "@tanstack/react-devtools": "^0.2.2",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.130.2",
    "@tanstack/react-router-devtools": "^1.131.5",
    "@tanstack/router-plugin": "^1.121.2",
    "@tiptap/extension-code-block-lowlight": "^3.6.6",
    "@tiptap/extension-link": "^3.6.6",
    "@tiptap/react": "^3.6.6",
    "@tiptap/starter-kit": "^3.6.6",
    "@use-funnel/browser": "^0.0.15",
    "ai": "^5.0.47",
    "lowlight": "^3.3.0",
    "marked": "^16.4.0",
    "nuqs": "^2.7.2",
    "openapi-fetch": "^0.14.0",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tailwindcss": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "jsdom": "^26.0.0",
    "openapi-typescript": "^7.9.1",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "web-vitals": "^4.2.4"
  }
}
