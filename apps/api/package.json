{
  "name": "api",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node ./dist/src/index.js",
    "vercel:dev": "npm run build && vercel dev",
    "build": "tsc",
    "check-types": "tsc --noEmit",
    "deploy": "vercel",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.14",
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@hono/node-server": "^1.19.3",
    "@hono/zod-openapi": "^1.1.0",
    "@neondatabase/serverless": "catalog:",
    "@repo/ai-types": "workspace:*",
    "@repo/api-spec": "workspace:*",
    "@repo/database": "workspace:*",
    "@scalar/hono-api-reference": "^0.9.18",
    "@types/bcryptjs": "^2.4.6",
    "@types/ws": "catalog:",
    "ai": "^5.0.47",
    "arctic": "^3.7.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.2",
    "drizzle-orm": "catalog:",
    "drizzle-zod": "^0.8.3",
    "file-type": "^21.0.0",
    "hono": "^4.9.7",
    "http-status": "^2.1.0",
    "http-status-codes": "^2.3.0",
    "jose": "^6.1.0",
    "nanoid": "^5.1.5",
    "pdf-parse": "^1.1.1",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "resend": "^6.1.0",
    "ws": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "tsx": "catalog:",
    "vercel": "^48.0.3"
  }
