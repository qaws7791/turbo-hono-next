{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && tailwindcss -i ./src/styles.css -o ./dist/styles.css --minify",
    "dev": "tsup --watch & tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@tailwindcss/postcss": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "autoprefixer": "^10.4.21",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "rimraf": "^6.0.1",
    "tailwindcss": "catalog:",
    "tsup": "^8.0.0",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@repo/ai-types": "workspace:*",
    "@tabler/icons-react": "^3.36.0",
    "@tailwindcss/cli": "^4.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "postcss": "^8.5.6",
    "react-day-picker": "^9.13.0",
    "react-resizable-panels": "^4.0.8",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "catalog:",
    "tw-animate-css": "^1.4.0",
    "vaul": "^1.1.2"
  },
  "peerDependencies": {
    "tailwind-variants": "catalog:"
  }
}
