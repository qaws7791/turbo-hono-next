{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/components/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tailwindcss -i ./src/styles/components.css -o ./dist/styles/components.css --minify",
    "dev": "tailwindcss -i ./src/styles/components.css -o ./dist/styles/components.css --watch",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@tailwindcss/postcss": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "autoprefixer": "^10.4.21",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "rimraf": "^6.0.1",
    "tailwindcss": "catalog:",
    "tsup": "^8.0.0",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@repo/ai-types": "workspace:*",
    "@tailwindcss/cli": "^4.1.13",
    "postcss": "^8.5.6",
    "tailwind-variants": "catalog:"
  },
  "peerDependencies": {
    "tailwind-variants": "catalog:"
  }
}
