{
  "name": "@repo/ai-types",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./message": {
      "types": "./src/message.ts",
      "default": "./src/message.ts"
    },
    "./metadata": {
      "types": "./src/metadata.ts",
      "default": "./src/metadata.ts"
    },
    "./data-parts": {
      "types": "./src/data-parts.ts",
      "default": "./src/data-parts.ts"
    },
    "./tools": {
      "types": "./src/tools.ts",
      "default": "./src/tools.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "ai": "^5.0.47",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "typesVersions": {
    "*": {
      "message": [
        "src/message.ts"
      ],
      "metadata": [
        "src/metadata.ts"
      ],
      "data-parts": [
        "src/data-parts.ts"
      ],
      "tools": [
        "src/tools.ts"
      ]
    }
  }
}
