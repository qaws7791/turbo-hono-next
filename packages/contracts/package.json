{
  "name": "@repo/contracts",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./common": {
      "types": "./src/common/schema.ts",
      "default": "./src/common/schema.ts"
    },
    "./auth": {
      "types": "./src/modules/auth/schema.ts",
      "default": "./src/modules/auth/schema.ts"
    },
    "./chat": {
      "types": "./src/modules/chat/schema.ts",
      "default": "./src/modules/chat/schema.ts"
    },
    "./materials": {
      "types": "./src/modules/materials/schema.ts",
      "default": "./src/modules/materials/schema.ts"
    },
    "./plans": {
      "types": "./src/modules/plans/schema.ts",
      "default": "./src/modules/plans/schema.ts"
    },
    "./sessions": {
      "types": "./src/modules/sessions/schema.ts",
      "default": "./src/modules/sessions/schema.ts"
    }
  },
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "dependencies": {
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}
